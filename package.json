  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theianjohnson/expo-sqlite-eloquent-orm/issues"
  },
  "homepage": "https://github.com/theianjohnson/expo-sqlite-eloquent-orm#readme",
  "devDependencies": {
    "dts-bundle-generator": "^8.1.2",
    "esbuild": "^0.19.5",
    "terser": "^5.24.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "expo-sqlite": "^11.6.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-native": "^0.72.6"
