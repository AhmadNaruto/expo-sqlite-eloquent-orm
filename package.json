{
  "name": "expo-sqlite-eloquent-orm",
  "version": "1.0.0",
  "description": "An Expo SQLite ORM",
  "main": "dist/bundle.min.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "tsc && esbuild src/index.ts --bundle --minify --outfile=dist/bundle.min.js --external:*",
